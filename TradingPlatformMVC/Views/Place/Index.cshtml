@model IEnumerable<TradingPlatformMVC.Models.place>

@{
    ViewBag.Title = "Places";
}

    <h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            City
        </th>
        <th>
            Country
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.city)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.country)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.place_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.place_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.place_id })
        </td>
    </tr>
}

</table>
